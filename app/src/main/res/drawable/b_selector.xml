<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>

            <solid
                android:color="#eff551" />
            <stroke
                android:width="1dp"
                android:color="#e6f8f9" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#d4e6f0"
                android:centerColor="#ffffff"
                android:endColor="#f5f5f5"
                android:angle="90" />
            <stroke
                android:width="2dp"
                android:color="#008b8b" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>