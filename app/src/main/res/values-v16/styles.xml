<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/MyGr</item>
        <item name="colorPrimaryDark">@color/Teal</item>
        <item name="colorAccent">@color/DarkGoldenrod</item>
        <item name="android:fontFamily">@font/snv_becker</item>
        <item name="android:textColor">#000000</item>
        <item name="android:actionOverflowButtonStyle">@style/MyActionButtonOverflow</item>

    </style>
    <style name="MyActionButtonOverflow" parent="android:style/Widget.Holo.Light.ActionButton.Overflow">
        <item name="android:src">@drawable/icon_menu</item>
        <item name="android:background">?android:attr/actionBarItemBackground</item>
    </style>
  
</resources>